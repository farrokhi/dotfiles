insert_final_newline = true
m_trailing_whitespace = true

[*.py]
indent_style = space
indent_size = 4
charset = utf-8

[*.c]
indent_size = 4

[Makefile]
indent_style = tab
indent_size = 8

[*.md]
trim_trailing_whitespace = false
indent_style = space
indent_size = 4

